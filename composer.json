{
    "name": "deetrych/mapping-bundle",
    "description": "Symfony bundle for mapping library",
    "authors": [
        {
            "name": "Mateusz",
            "email": "mateusz.ditrich@yahoo.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/matigda/mapping.git"
        }
    ],
    "require": {
        "php": ">=7.0.1",
        "deetrych/mapping": "dev-master",
        "symfony/framework-bundle": "^2.7 || ^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "symfony/yaml": "^2.7 || ^3.1"
    },
    "autoload": {
        "psr-4": { "Deetrych\\MappingBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": {
            "Deetrych\\MappingBundle\\Tests\\": "Tests/"
        }
    }
}
